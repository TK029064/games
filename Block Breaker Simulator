repeat task.wait() until game:IsLoaded() 

local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")() -- Self explaining

_G.closeBind = Enum.KeyCode.LeftAlt

local win = lib:Window("BBS", "Strike Hub", Color3.fromRGB(255, 25, 84), _G.closeBind) -- _G.closeBind is the bind to close the gui, kinda cool for a bind in your GUI

local tab = win:Tab("Home", "rbxassetid://6026568198") -- Logo is the link
local CoreGui = game:GetService("StarterGui") 
local vu = game:GetService("VirtualUser")
egg = {}
for i,v in pairs(game:GetService("Workspace").EggVendors:GetChildren()) do
    if v.Name ~= "UltimateWeaponsEgg" then
        table.insert(egg,v)
    end
end

tab:Line()

tab:Button("Click to Copy Discord Server", "Join to know what the news", function() -- Description is the text you'll see when you click on the arrow at the right of the button
 setclipboard("https://discord.gg/PRRwyEZ9FH")
lib:Notification("Link Set to clipboard", "Paste the link in your browser") -- Example of a notification on the GUI (optional)
end) -- Example of a notification on the GUI (optional)

tab:Label("Credit: TK02#9064") -- Self explaining
tab:Label("Open/Close Ui: LeftAlt")

tab:Line() -- Put nothing it's just a bar
local tab = win:Tab("Automatic", "rbxassetid://9421664858") -- Logo is the link
tab:Line()

tab:Label("AutoFarm")

tab:Toggle("Inf Money", "Wait Few secondes ", false, function(t) -- t can be anything, choose false or true for what is the value in default
    shared.toggle = t
    Money = t
    print(Money,t)
end)

tab:Toggle("Auto ThrowPickaxe", "EZ", false, function(t) -- t can be anything, choose false or true for what is the value in default
    shared.toggle = t
    ThrowPickaxe = t
    print(ThrowPickaxe,t)
end)

tab:Toggle("Auto Upgrade All", "EZ", false, function(t) -- t can be anything, choose false or true for what is the value in default
    shared.toggle = t
    UpgradeAll = t
    print(UpgradeAll,t)
end)

tab:Toggle("Auto Rebirth", "EZ", false, function(t) -- t can be anything, choose false or true for what is the value in default
    shared.toggle = t
    Rebirth = t
    print(Rebirth,t)
end)

tab:Line()
tab:Label("Eggs")


local Enjoy = tab:Dropdown("Auto Egg", {"1", "2", "3", "4", "5", "6", "7", "8"}, function(t)
    SelectedEgg = t
    print(SelectedEgg)
end)

tab:Toggle("Auto egg", "EZ", false, function(t) -- t can be anything, choose false or true for what is the value in default
    shared.toggle = t
    AutoEgg = t
    print(AutoEgg,t)
end)
local tab = win:Tab("Player", "http://www.roblox.com/asset/?id=6023426915")

tab:Slider("WalkSpeed", "EZ", 0, 500, 16, function(t) -- 0 is the minimum, 100 is the maximum, 50 is the base value <--(it's glitchy for me with negative limits)
   game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = t
end)

tab:Button("Reset Walk and Jump", "EZ", function() -- Description is the text you'll see when you click on the arrow at the right of the button
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
lib:Notification("You are Normal Again now", "Okay") 
end)



-- functions
spawn(function()
    while wait() do
        if Money == true then
            game:GetService("ReplicatedStorage").Events.ClientBrokeBlocks:FireServer(math.huge)
            game:GetService("ReplicatedStorage").Events.SellBlocks:FireServer()
        end
    end
end)
spawn(function()
    while wait() do
        if ThrowPickaxe == true then
            game:GetService("ReplicatedStorage").Events.ThrowPickaxe:FireServer()
        end
    end
end)

spawn(function()
    while wait(.1) do
        if UpgradeAll == true then
            game:GetService("ReplicatedStorage").Events.PlayerUpgradeAxe:FireServer("NumberOfAxes")    
            game:GetService("ReplicatedStorage").Events.PlayerUpgradeAxe:FireServer("BackpackSize")  
            game:GetService("ReplicatedStorage").Events.PlayerUpgradeAxe:FireServer("AxeDamage")   
            game:GetService("ReplicatedStorage").Events.PlayerUpgradeAxe:FireServer("AxeRange")
            game:GetService("ReplicatedStorage").Events.PlayerUpgradeAxe:FireServer("ThrowCooldown")
            game:GetService("ReplicatedStorage").Events.PlayerUpgradeAxe:FireServer("AxeEvolution")
        end
    end
end)
spawn(function()
    while wait() do
        if Rebirth == true then
            game:GetService("ReplicatedStorage").Events.PlayerRebirth:FireServer()
        end
    end
end)
spawn(function()
    while wait() do
        if AutoEgg == true then
            local args = {
                [1] = tostring(SelectedEgg)
            }
            
            game:GetService("ReplicatedStorage").Events.PlayerPressedKeyOnEgg:FireServer(unpack(args))
        end
    end
end)

CoreGui:SetCore("SendNotification", {
    Title = "Game Suppoted",
    Text = "Script Loaded",
   Duration = 3;
   })
