repeat task.wait() until game:IsLoaded() 

local CoreGui = game:GetService("StarterGui") 
CoreGui:SetCore("SendNotification", {
 Title = "Game Suppoted",
 Text = "Script Loaded",
Duration = 5;
})


local lib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")() -- Self explaining

_G.closeBind = Enum.KeyCode.LeftAlt

local win = lib:Window("Strong Clickers", "Strike Hub", Color3.fromRGB(255, 25, 84), _G.closeBind) -- _G.closeBind is the bind to close the gui, kinda cool for a bind in your GUI

local tab = win:Tab("Home", "rbxassetid://6026568198") -- Logo is the link
egg = {}
for i,v in pairs(game:GetService("Workspace").Eggs:GetChildren()) do
    table.insert(egg,v)
end
world = {}
for i,v in pairs(game:GetService("Workspace").Portals.Locations:GetChildren()) do
    table.insert(world,v)
end
tab:Line()

tab:Button("Click to Copy Discord Server", "Join to know what the news", function() -- Description is the text you'll see when you click on the arrow at the right of the button
 setclipboard("https://discord.gg/PRRwyEZ9FH")
lib:Notification("Link Set to clipboard", "Paste the link in your browser") -- Example of a notification on the GUI (optional)
end) -- Example of a notification on the GUI (optional)

tab:Label("Credit: TK02#9064") -- Self explaining
tab:Label("Open/Close Ui: LeftAlt")

tab:Line() -- Put nothing it's just a bar
local tab = win:Tab("Automatic", "rbxassetid://9421664858") -- Logo is the link
tab:Line()

tab:Label("AutoFarm")
tab:Button("Remove Some Popups", "EZ", function() -- Description is the text you'll see when you click on the arrow at the right of the button
    pcall(function() game:GetService("Players").LocalPlayer.PlayerGui.Interface.ScreenDebris:Destroy() end)
lib:Notification("Popups Removed", "ok") -- Example of a notification on the GUI (optional)
end)  -- Example of a notification on the GUI (optional)

tab:Toggle("Auto click", "EZ", false, function(t) -- t can be anything, choose false or true for what is the value in default
    shared.toggle = t
    Strength = t
    print(Strength,t)
end)

tab:Toggle("Auto Sell", "EZ", false, function(t) -- t can be anything, choose false or true for what is the value in default
        _G.autoSeller = t
        if t then
            autoSell()
	end    
end)

tab:Toggle("Auto Rebirth", "will auto merge all pets", false, function(t) -- t can be anything, choose false or true for what is the value in default
        _G.autoRebirther = t
        if t then
            autoRebirth()
	end    
end)
tab:Line()
tab:Label("Eggs")

tab:Toggle("Auto egg", "EZ", false, function(t) -- t can be anything, choose false or true for what is the value in default
    shared.toggle = t
    AutoEgg = t
    print(AutoEgg,t)
end)

local Enjoy = tab:Dropdown("Auto Egg", {"Basic Egg", "Deadly Egg", "Earthly Egg", "Mountain Egg", "Desert Egg", "Winter Egg", "Beach Egg", "Ocean Egg", "Moon Egg", "Tech Egg"}, function(t)
    SelectedEgg = t
    print(SelectedEgg)
end)

tab:Line()
local tab = win:Tab("Player", "http://www.roblox.com/asset/?id=6023426915")

tab:Slider("WalkSpeed", "EZ", 0, 500, 16, function(t) -- 0 is the minimum, 100 is the maximum, 50 is the base value <--(it's glitchy for me with negative limits)
   game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = t
end)
tab:Slider("JumpPower", "EZ", 0, 500, 50, function(t) -- 0 is the minimum, 100 is the maximum, 50 is the base value <--(it's glitchy for me with negative limits)
   game.Players.LocalPlayer.Character.Humanoid.JumpPower = t
end)
tab:Button("Reset Walk and Jump", "EZ", function() -- Description is the text you'll see when you click on the arrow at the right of the button
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
lib:Notification("You are Normal Again now", "Okay") 
end)

local tab = win:Tab("Teleport", "rbxassetid://6034503364")

local Enjoy = tab:Dropdown("Selecte The World", {"Plains", "Mountains", "Desert", "Winter", "Beach", "Ocean", "Moon", "Tech"}, function(t)
    SelectedEWorld = t
    print(SelectedEWorld)
end)

tab:Button("Tp Selected World", "EZ", function() -- Description is the text you'll see when you click on the arrow at the right of the button
   game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Portals.Locations[tostring(SelectedEWorld)].CFrame
end)
tab:Line()
tab:Button("Tp Spawn", "EZ", function() -- Description is the text you'll see when you click on the arrow at the right of the button
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Portals.ReturnSpawn.CFrame
end)

tab:Button("Tp Upgrades Shop", "EZ", function() -- Description is the text you'll see when you click on the arrow at the right of the button
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").TriggerAreas.Upgrades.Platform.CFrame
end)

-- functions
spawn(function()
    while wait() do
        if Strength == true then
            for i = 1,5 do
                game:GetService("ReplicatedStorage").Remotes.TrainStrength:FireServer()
            end
        end
    end
end)
function autoRebirth()
    spawn(function()
        while _G.autoRebirther == true do
            game:GetService("ReplicatedStorage").Remotes.Rebirth:FireServer()
            wait(1)
        end
    end)
end

local plr = game.Players.LocalPlayer.Character.Head
function autoSell()
	spawn(function()
		while _G.autoSeller == true do
			for _,v in pairs(game:GetService("Workspace").TriggerAreas:GetDescendants()) do
  			if v:IsA("TouchTransmitter") then
    			firetouchinterest(plr, v.Parent, 0)
    			task.wait()
    			firetouchinterest(plr, v.Parent, 1)
			end
		end
	end
end)
end
spawn(function()
    while wait() do
        if AutoEgg == true then
            local args = {
                [1] = tostring(SelectedEgg)
            }
            
            game:GetService("ReplicatedStorage").Remotes.HatchEgg:FireServer(unpack(args))
        end
    end
end)
local vu = game:GetService("VirtualUser")

game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
